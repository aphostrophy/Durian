{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "files.associations": {
    "bpf.h": "c",
    "bpf_load.h": "c",
    "trace_helpers.h": "c",
    "bpf_tracing.h": "c",
    "bpf_helpers.h": "c",
    "version.h": "c",
    "typeinfo": "c",
    "libbpf.h": "c",
    "errno.h": "c",
    "stdio.h": "c",
    "stddef.h": "c",
    "stdarg.h": "c",
    "oberon_loader.h": "c",
    "oberon_maps.h": "c",
    "assert.h": "c",
    "bpf_core_read.h": "c",
    "bpf_helper_defs.h": "c",
    "bitset": "c",
    "oberon_common_user_bpf.h": "c",
    "oberon_common_user_debug.h": "c",
    "random": "c",
    "hiredis.h": "c",
    "oberon_def.h": "c",
    "oberon_sched_stats_transactions.h": "c",
    "cstring": "c",
    "oberon_common_repository.h": "c",
    "oberon_def_interface.h": "c"
  },
  "C_Cpp.errorSquiggles": "enabledIfIncludesResolve",
  "C_Cpp.files.exclude": {
    "**/oberon": true,
    "**/.vscode": true
  },
  "cmake.configureOnOpen": false
}
